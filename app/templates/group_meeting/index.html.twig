{% extends 'base.html.twig' %}

{% block title %}Hello GroupMeetingController!{% endblock %}

{% block body %}

<div class="example-wrapper">
    <h1>Group Meetings</h1>

    <ul class="group-meetings app-list fa-ul">
        {% for group_meeting in group_meetings %}
            <li><i class="fa fa-calendar" aria-hidden="true"></i>
                <a href="{{ path('group_meeting', {fileId: group_meeting.fileId}) }}">
                    {{ group_meeting.name }}
                </a>
            </li>
        {% endfor %}
    </ul>

{#    <details>#}
